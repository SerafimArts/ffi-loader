{
    "name": "serafim/ffi-loader",
    "description": "FFI libraries loader with simple preprocessor",
    "license": "MIT",
    "keywords": [
        "ffi",
        "parser",
        "compiler",
        "loader"
    ],
    "require": {
        "php": ">=8.0",
        "ext-ffi": "*",
        "ext-iconv": "*",
        "ext-mbstring": "*",
        "serafim/flux": "^1.0",
        "serafim/endianness": "^1.0",
        "psr/log": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Serafim\\FFILoader\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.1",
        "symfony/var-dumper": "^5.0",
        "vimeo/psalm": "^4.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Example\\": "example",
            "Serafim\\FFILoader\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
